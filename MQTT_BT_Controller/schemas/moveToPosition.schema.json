{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "title": "Move to Position Request",
    "subtopic": "/XYMotion",
    "type": "object",
    "properties": {
        "XbotId": {
            "type": "integer",
            "description": "ID of the xBot to move"
        },
        "TargetPos": {
            "type": "array",
            "description": "Target position to move the xBot to [x, y]",
            "items": {
            "type": "number"
            },
            "minItems": 2,
            "maxItems": 2
        },
        "CommandUuid": {
            "type": "string",
            "description": "Unique identifier for the request"
        }
    },
    "required": ["XbotId", "TargetPos", "CommandUuid"]
}