                          "modelType": "MultiLanguageProperty"
                        },
                        {
                          "idShort": "type",
                          "semanticId": {
                            "type": "ExternalReference",
                            "keys": [{
                              "type": "GlobalReference",
                              "value": "https://www.w3.org/1999/02/22-rdf-syntax-ns#type"
                            }]
                          },
