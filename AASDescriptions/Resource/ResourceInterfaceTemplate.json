    {
      "idShort": "Variables",
      "id": "https://smartproductionlab.aau.dk/submodels/templates/Variables",
      "kind": "Template",
      "semanticId": {
        "type": "ExternalReference",
        "keys": [{"type": "GlobalReference", "value": "https://admin-shell.io/idta/Variables/1/0/Submodel"}]
      },
      "description": [
        {
          "language": "en",
          "text": "Optional submodel for resource runtime variables and state information. Resource instances can add state variables, occupancy info, errors, etc."
        }
      ],
      "submodelElements": [
        {
          "idShort": "exampleStateVariable",
          "description": [
            {
              "language": "en",
              "text": "Example state variable. Resource instances replace this with their actual state variables (e.g., PackMLState, OccupationState, ErrorState)"
            }
          ],
          "modelType": "Property",
          "valueType": "xs:string"
        },
        {
          "idShort": "exampleStateCollection",
          "description": [
            {
              "language": "en",
              "text": "Example for grouped state information. Use SubmodelElementCollection for complex state structures"
            }
          ],
          "modelType": "SubmodelElementCollection",
          "value": [
            {
              "idShort": "State",
              "description": [{"language": "en", "text": "State value or status"}],
              "modelType": "Property",
              "valueType": "xs:string"
            },
            {
              "idShort": "Timestamp",
              "description": [{"language": "en", "text": "When the state was last updated"}],
              "modelType": "Property",
              "valueType": "xs:dateTime"
            }
          ]
        },
        {
          "idShort": "exampleMetricVariable",
          "description": [
            {
              "language": "en",
              "text": "Example numeric variable for measurements or metrics (e.g., PowerConsumption, Temperature, Pressure)"
            }
          ],
          "modelType": "Property",
          "valueType": "xs:double"
        },
        {
          "idShort": "examplePropertyWithInterface",
          "description": [
            {
              "language": "en",
              "text": "Example property that has an associated interface definition. Use SubmodelElementCollection to group the value with its interface reference."
            }
          ],
          "modelType": "SubmodelElementCollection",
          "value": [
            {
              "idShort": "Value",
              "description": [{"language": "en", "text": "The actual runtime value of the property"}],
              "modelType": "Property",
              "valueType": "xs:double"
            },
            {
              "idShort": "InterfaceReference",
              "semanticId": {
                "type": "ExternalReference",
                "keys": [{
                  "type": "GlobalReference",
                  "value": "https://admin-shell.io/idta/AssetInterfacesDescription/1/0/InterfaceReference"
                }]
              },
              "description": [
                {
                  "language": "en",
                  "text": "Reference to the property's interface definition in AssetInterfacesDescription. This links the runtime value to its access protocol/API."
                }
              ],
              "modelType": "ReferenceElement",
              "value": {
                "type": "ModelReference",
                "keys": [
                  {
                    "type": "Submodel",
                    "value": "https://smartproductionlab.aau.dk/submodels/instances/{resourceId}/AssetInterfacesDescription"
                  },
                  {
                    "type": "SubmodelElementCollection",
                    "value": "Interface{ProtocolName}"
                  },
                  {
                    "type": "SubmodelElementCollection",
                    "value": "InteractionMetadata"
                  },
                  {
                    "type": "SubmodelElementCollection",
                    "value": "properties"
                  },
                  {
                    "type": "SubmodelElementCollection",
                    "value": "{propertyName}"
                  }
                ]
              }
            }
          ]
        }
      ],
      "modelType": "Submodel"
