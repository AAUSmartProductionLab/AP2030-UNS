                          "description": [{"language": "en", "text": "Unique identifier for the property"}],
                          "modelType": "Property",
                          "valueType": "xs:string"
                        },
                        {
                          "idShort": "title",
                          "description": [{"language": "en", "text": "Human-readable name"}],
                          "modelType": "Property",
                          "valueType": "xs:string"
                        },
                        {
                          "idShort": "description",
