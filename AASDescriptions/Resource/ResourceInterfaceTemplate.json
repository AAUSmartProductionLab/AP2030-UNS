                      "description": [
                        {
                          "language": "en", 
                          "text": "Example property structure. Resource instances replace this with their actual properties (e.g., weight, temperature, position)"
                        }
                      ],
                      "modelType": "SubmodelElementCollection",
                      "value": [
                        {
                          "idShort": "key",
                          "semanticId": {
                            "type": "ExternalReference",
                            "keys": [{
                              "type": "GlobalReference",
                              "value": "https://admin-shell.io/idta/AssetInterfacesDescription/1/0/key"
                            }]
