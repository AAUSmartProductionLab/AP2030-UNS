  "submodels": [
    {
      "idShort": "AssetInterfacesDescription",
      "id": "https://smartproductionlab.aau.dk/submodels/templates/AssetInterfacesDescription",
      "kind": "Template",
      "semanticId": {
        "type": "ExternalReference",
        "keys": [{"type": "GlobalReference", "value": "https://admin-shell.io/idta/AssetInterfacesDescription/1/0/Submodel"}]
      },
      "description": [
        {
          "language": "en",
          "text": "Generic template for resource interface descriptions following IDTA 02017-1-0 and W3C WoT Thing Description. Defines the structural pattern for any resource (filling systems, cameras, AMRs, etc.)"
        }
      ],
      "submodelElements": [
        {
          "idShort": "Interface",
          "semanticId": {
            "type": "ExternalReference",
            "keys": [{
              "type": "GlobalReference",
              "value": "https://admin-shell.io/idta/AssetInterfacesDescription/1/0/Interface"
            }]
          },
          "description": [
            {
              "language": "en",
              "text": "Interface definition. Each resource instance must specify the protocol (HTTP, MQTT, OPC UA, etc.) via supplementalSemanticIds"
            }
          ],
          "modelType": "SubmodelElementCollection",
          "value": [
            {
              "idShort": "title",
              "description": [{"language": "en", "text": "Human-readable title for the interface"}],
              "modelType": "Property",
              "valueType": "xs:string"
            },
            {
              "idShort": "EndpointMetadata",
              "semanticId": {
                "type": "ExternalReference",
                "keys": [{
                  "type": "GlobalReference",
                  "value": "https://admin-shell.io/idta/AssetInterfacesDescription/1/0/EndpointMetadata"
                }]
              },
              "description": [
                {
                  "language": "en", 
                  "text": "Protocol-specific endpoint configuration. Properties depend on the protocol (base URI for MQTT/HTTP, endpoint URL for OPC UA, etc.)"
                }
              ],
              "modelType": "SubmodelElementCollection",
              "value": [
                {
                  "idShort": "base",
                  "description": [{"language": "en", "text": "Base URI or endpoint address for the protocol"}],
                  "modelType": "Property",
                  "valueType": "xs:string"
                },
                {
                  "idShort": "contentType",
                  "description": [{"language": "en", "text": "Default content type for messages (e.g., application/json, application/xml)"}],
                  "modelType": "Property",
                  "valueType": "xs:string"
                }
              ]
            },
            {
              "idShort": "InteractionMetadata",
              "semanticId": {
                "type": "ExternalReference",
                "keys": [{
                  "type": "GlobalReference",
                  "value": "https://admin-shell.io/idta/AssetInterfacesDescription/1/0/InteractionMetadata"
                }]
              },
              "supplementalSemanticIds": [{
                "type": "ExternalReference",
                "keys": [{
                  "type": "GlobalReference",
                  "value": "https://www.w3.org/2019/wot/td#InteractionAffordance"
                }]
              }],
              "description": [
                {
                  "language": "en", 
                  "text": "Container for all available interactions. Resource instances populate this with their specific actions, properties, and events"
                }
              ],
              "modelType": "SubmodelElementCollection",
              "value": [
                {
                  "idShort": "actions",
                  "semanticId": {
                    "type": "ExternalReference",
                    "keys": [{
                      "type": "GlobalReference",
                      "value": "https://www.w3.org/2019/wot/td#ActionAffordance"
                    }]
                  },
                  "description": [
                    {
                      "language": "en", 
                      "text": "Collection of invokable actions. Each action must follow the ActionDefinition structure below"
                    }
                  ],
                  "modelType": "SubmodelElementCollection",
                  "value": [
                    {
                      "idShort": "exampleAction",
                      "semanticId": {
                        "type": "ExternalReference",
                        "keys": [{
                          "type": "GlobalReference",
                          "value": "https://admin-shell.io/idta/AssetInterfaceDescription/1/0/ActionDefinition"
                        }]
                      },
                      "supplementalSemanticIds": [{
                        "type": "ExternalReference",
                        "keys": [{
                          "type": "GlobalReference",
                          "value": "https://www.w3.org/2019/wot/td#name"
                        }]
                      }],
                      "description": [
                        {
                          "language": "en", 
                          "text": "Example action structure. Resource instances replace this with their actual actions (e.g., dispense, capture, moveTo)"
                        }
                      ],
                      "modelType": "SubmodelElementCollection",
                      "value": [
                        {
                          "idShort": "key", 
                          "description": [{"language": "en", "text": "Unique identifier for the action"}], 
                          "modelType": "Property", 
                          "valueType": "xs:string"
                        },
                        {
                          "idShort": "title", 
                          "description": [{"language": "en", "text": "Human-readable name"}], 
                          "modelType": "Property", 
                          "valueType": "xs:string"
                        },
                        {
                          "idShort": "description",
                          "description": [{"language": "en", "text": "Detailed description of what the action does"}],
                          "modelType": "MultiLanguageProperty"
                        },
                        {
                          "idShort": "synchronous", 
                          "description": [{"language": "en", "text": "true = waits for completion, false = fire-and-forget"}], 
                          "modelType": "Property", 
                          "valueType": "xs:boolean"
                        },
                        {
                          "idShort": "input",
                          "description": [{"language": "en", "text": "JSON Schema reference or inline schema for input validation"}],
                          "modelType": "File",
                          "contentType": "application/schema+json"
                        },
                        {
                          "idShort": "output",
                          "description": [{"language": "en", "text": "JSON Schema reference or inline schema for output validation"}],
                          "modelType": "File",
                          "contentType": "application/schema+json"
                        },
                        {
                          "idShort": "forms",
                          "semanticId": {
                            "type": "ExternalReference",
                            "keys": [{
                              "type": "GlobalReference",
                              "value": "https://www.w3.org/2019/wot/td#Form"
                            }]
                          },
                          "description": [
                            {
                              "language": "en", 
                              "text": "Protocol-specific bindings. Structure varies by protocol (MQTT: topics & QoS, HTTP: methods & URLs, OPC UA: node IDs)"
                            }
                          ],
                          "modelType": "SubmodelElementCollection",
                          "value": [
                            {
                              "idShort": "contentType", 
                              "description": [{"language": "en", "text": "Content type for this specific form"}],
                              "modelType": "Property", 
                              "valueType": "xs:string"
                            },
                            {
                              "idShort": "href", 
                              "description": [{"language": "en", "text": "Protocol-specific address (topic, URL, node ID)"}], 
                              "modelType": "Property", 
                              "valueType": "xs:string"
                            },
                            {
                              "idShort": "op", 
                              "description": [{"language": "en", "text": "Operation type (invokeaction, readproperty, writeproperty, etc.)"}], 
                              "modelType": "Property", 
                              "valueType": "xs:string"
                            }
                          ]
                        }
                      ]
                    }
                  ]
                },
                {
                  "idShort": "properties",
                  "semanticId": {
                    "type": "ExternalReference",
                    "keys": [{
                      "type": "GlobalReference",
                      "value": "https://www.w3.org/2019/wot/td#PropertyAffordance"
                    }]
                  },
                  "description": [
                    {
                      "language": "en", 
